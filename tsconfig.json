{
  "compilerOptions": {
    "target": "ESNext",
    "jsx": "react",
    "module": "commonjs",
    "baseUrl": ".",
    "paths": {
      "@web/*": ["./web/*"]
    },
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
