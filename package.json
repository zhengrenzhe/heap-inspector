{
  "name": "heap-visualization",
  "version": "0.1.0",
  "scripts": {
    "build": "npm run clean && npm run build-wasm && npm run build-web",
    "build-wasm": "wasm-pack build --target web --mode normal --release",
    "build-web": "NODE_ENV=production webpack -c webpack.config.ts",
    "dev": "NODE_ENV=development webpack-dev-server -c webpack.config.ts",
    "clean": "rimraf dist pkg",
    "install-env": "cargo install wasm-pack && npm i"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.174",
    "@types/node": "^17.0.16",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/webpack": "^5.28.0",
    "@types/webpack-bundle-analyzer": "^4.4.1",
    "@types/wicg-file-system-access": "^2020.9.5",
    "@wasm-tool/wasm-pack-plugin": "^1.6.0",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.6.0",
    "html-webpack-plugin": "^5.5.0",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5",
    "webpack": "^5.68.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "dependencies": {
    "@antv/g6": "^4.5.4",
    "@mantine/core": "^3.6.6",
    "@mantine/hooks": "^3.6.6",
    "comlink": "^4.3.1",
    "mobx": "^6.3.13",
    "mobx-react": "^7.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.6.0"
  }
}
