{
  "name": "heap-inspector",
  "displayName": "Heap Inspector",
  "version": "1.0.0",
  "description": "heap inspector",
  "scripts": {
    "watch": "NODE_ENV=development vite build  --config vite.config.ts --watch ",
    "dev": "vite --port=3000 --config vite.config.ts",
    "build": "rimraf dist && vite build --config vite.config.ts",
    "compress-web": "brotli-cli compress -v -m=text -q=11 --br=false dist/*",
    "release": "npm run build && npm run compress-web && cargo build --release"
